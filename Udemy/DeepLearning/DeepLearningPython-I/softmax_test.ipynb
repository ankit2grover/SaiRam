   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.random.randn(100, 5)\n",
    "A = np.exp(A)\n",
    "sum = np.sum(A, axis = 1, keepdims = True)\n",
    "softmax = A / sum \n",
    "verify_output = np.sum(softmax, axis = 1)\n",
    "print verify_output\n"
   ]
